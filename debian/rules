#!/usr/bin/make -f

include /usr/share/dpkg/pkg-info.mk
include /usr/share/dpkg/architecture.mk
-include .github/local/rules.local

%:
	dh $@

override_dh_builddeb:
	dh_builddeb -- -Zxz

override_dh_installudev:
	dh_installudev -pradxa-system-config-allwinner --name=enable_hid_wakeup --priority=60
	dh_installudev -pradxa-system-config-allwinner --name=hdmi-hotplug --priority=60

override_dh_installsystemd:
	dh_installsystemd --name=hdmi-toggle-once
